  "scripts": {
    "build": "nest build",
    "start": "dotenv -e .env.development nest start",
    "start:dev": "nest start --watch --type-check",
    "start:debug": "nest start --debug --watch",
    "start:staging": "dotenv -e .env.development node dist/src/main",
    "start:prod": "dotenv -e .env.production node dist/src/main",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "fix": "yarn lint:fix && yarn prettier:fix",
    "test": "dotenv -e .env.development jest",
    "test:watch": "dotenv -e .env.development jest --watch",
    "test:cov": "dotenv -e .env.development jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:create": "prisma migrate dev",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:indexing": "ts-node prisma/partial-indexes.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.2",
