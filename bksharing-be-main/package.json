    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.4.15",
    "@prisma/client": "^5.19.0",
    "@stream-io/node-sdk": "^0.4.4",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.21",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.5.0",
